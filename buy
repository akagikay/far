loadstring(game:HttpGet("https://raw.githubusercontent.com/Xenijo/AdoptMe-RemoteBypass/main/Bypass.lua"))()
wait(100)
-- This loop will run the code inside of it 100 times.
for i = 1, 100 do
    -- The code you provided is placed within the loop.
    local args = {
        "pets",
        "house_pets_2025_siamese_cat",
        {
            buy_count = 1
        }
    }
    game:GetService("ReplicatedStorage"):WaitForChild("API"):WaitForChild("brzANdXPSLQcN/I"):InvokeServer(unpack(args))
    
    -- It is recommended to add a small delay to prevent potential issues.
    wait(1) 
end
