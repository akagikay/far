wait(30)
for i = 1, 100 do
    local args = {
        "pet_accessories",
        "blue_cat_ear_headphones",
        {
            buy_count = 1
        }
    }

    game:GetService("ReplicatedStorage")
        :WaitForChild("API")
        :WaitForChild("ShopAPI/BuyItem")
        :InvokeServer(unpack(args))

    -- optional small delay to avoid rate limits
    task.wait(0.1)
end
