wait(100)
local RS = game:GetService("ReplicatedStorage")
local buyItem = RS:WaitForChild("API"):WaitForChild("ShopAPI/BuyItem")

-- Buy spike collar 50 times
for i = 1, 50 do
    buyItem:InvokeServer(
        "pet_accessories",
        "spike_collar",
        { buy_count = 1 }
    )
    task.wait(0.1)
end

-- Buy black hightops 10 times
for i = 1, 10 do
    buyItem:InvokeServer(
        "pet_accessories",
        "black_hightops",
        { buy_count = 1 }
    )
    task.wait(0.1)
end

