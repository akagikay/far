local ReplicatedStorage = game:GetService("ReplicatedStorage")
local BuyItem = ReplicatedStorage:WaitForChild("API"):WaitForChild("ShopAPI/BuyItem")
for i = 1, 100 do
    BuyItem:InvokeServer(
        "pet_accessories",
        "bat_wings",
        { buy_count = 1 }
    )
    task.wait(0.1) -- optional delay to avoid spam / throttling
end
