local RunService = game:GetService("RunService")
local INTERVAL_SECONDS = 1800
local function cleanMemory()
    local startMem = gcinfo()

    collectgarbage("collect")
    
    local endMem = gcinfo()
    
    print(string.format("[Memory Cleaner]: Cleaned up. Freed %d KB. Current usage: %d KB", (startMem - endMem), endMem))
end
task.spawn(function()
    while true do
        task.wait(INTERVAL_SECONDS)
        cleanMemory()
    end
end)
